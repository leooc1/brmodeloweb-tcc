{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "app/dist"
      }
    }
  ],
  "devCommand": "yarn start:frontend",
  "routes": [
    { "src": "/(.*\\.(js|css|map|png|jpg|jpeg|svg|woff|woff2|json|ico))$", "dest": "/app/dist/$1" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/app/dist/index.html" }
  ]
}
